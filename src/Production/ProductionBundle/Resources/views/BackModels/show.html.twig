{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BackModels</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ entity.active }}</td>
            </tr>
            <tr>
                <th>Sort</th>
                <td>{{ entity.sort }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Mastersize</th>
                <td>{{ entity.masterSize }}</td>
            </tr>
            <tr>
                <th>New</th>
                <td>{{ entity.new }}</td>
            </tr>
            <tr>
                <th>Bag</th>
                <td>{{ entity.bag }}</td>
            </tr>
            <tr>
                <th>Box</th>
                <td>{{ entity.box }}</td>
            </tr>
            <tr>
                <th>Weightwhite</th>
                <td>{{ entity.weightWhite }}</td>
            </tr>
            <tr>
                <th>Weightcolor</th>
                <td>{{ entity.weightColor }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ entity.slug }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('backmodels') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('backmodels_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('backmodels_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
